{
  "tabWidth": 2,
  "singleQuote": false,
  "trailingComma": "all",
  "printWidth": 80,
  "bracketSpacing": true,
  "endOfLine": "auto",
  "importOrder": [
    "^(react/(.*)$)|^(react$)",
    "<THIRD_PARTY_MODULES>",
    "^pages/(.*)$",
    "^components/(.*)$",
    "^hooks/(.*)$",
    "^helpers/(.*)$",
    "^models/(.*)$",
    "^assets/(.*)$",
    "^shared/(.*)$",
    "^(\\.\\./[^.]*|\\.\\.)$",
    "^../(.*)$",
    "^./[^.]*$",
    "^./(.*)\\.(?!css|scss)$",
    "^.*\\.(css|scss)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
